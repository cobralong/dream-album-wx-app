page{
    background-color: #f5f5f5;
}
.resetNumber{
    height:120rpx;
    width:100%;
    position: relative;
    margin-top:20rpx;
    margin-bottom:20rpx;
    background-color: #fff;
}
.resetYqm{
    position: absolute;
    height:120rpx;
    text-align: center;
    line-height: 120rpx;
    left:20rpx;
}
.resetText{
    position: absolute;
    height:120rpx;
    line-height: 120rpx;
    text-align: center;
    left:calc(50% - 90rpx);
    color: mediumspringgreen;
}
.resetButtonImage{
    position: absolute;
    height:70rpx;
    width:130rpx;
    top:30rpx;
    right: 20rpx;
}
.friendsTop{
    position: relative;
    height:150rpx;
    width:100%;
    border-bottom: 1px solid #f5f5f5;
    display: flex;
    background-color: #fff;
}
.friends-avatarUrl{
    position: absolute;
    width: 120rpx;
    height: 120rpx;
    border-radius: 50%;
    border: 1px solid #fff;
    top:15rpx;
    left:15rpx;
}
.nickNameText{
    position: absolute;
    left:180rpx;
    display: flex;
    flex-direction: column;
    width:calc(100% - 270rpx);
    font-size: 36rpx;
    line-height: 37rpx;
    height:74rpx;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: pre-wrap;
    word-wrap: break-word;
}
.historyText{
    position: absolute;
    left:180rpx;
    display: flex;
    flex-direction: column;
    top:110rpx;
    width:calc(100% - 270rpx);
    font-size: 28rpx;
    line-height: 28rpx;
    color: #aaa;
}
.navigator{
    position: absolute;
    line-height: 150rpx;
    color: #aaa;
    text-align: center;
    width:50rpx;
    left:calc(100% - 50rpx);
}