.friendsTop{
    height:150rpx;
    width:100%;
    border-bottom: 1px solid #f5f5f5;
    display: flex;
}
.friends-avatarUrl{
    width: 120rpx;
    height: 120rpx;
    border-radius: 50%;
    border: 1px solid #fff;
    margin:15rpx;
}
.friends-nickName{
    display: flex;
    flex-direction: column;
    margin-top:30rpx;
    margin-left: 20rpx;
}
.nickNameText{
    font-size: 36rpx;
    line-height: 36rpx;
}
.historyText{
    font-size: 28rpx;
    margin-top:20rpx;
    color: #aaa;
}
.navigator{
    line-height: 150rpx;
    color: #aaa;
    text-align: right;
    width:250rpx;
}