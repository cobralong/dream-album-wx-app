<!--重置邀请码-->
<view wx:if="{{secert!='undefined' &&  secert!='null'}}" class="resetNumber">
    <view class="resetYqm">
        邀请码
    </view>
    <view class="resetText">
        {{secert}}
    </view>
    <image bindtap="resetYqm" class="resetButtonImage" src="../../image/reset.png" />
</view>

<!--浏览记录-->
<view bindtap="showDetail" data-openid="{{item.openId}}" class="friendsTop" wx:for="{{occupantList}}" wx:key="avatarUrlsTop">
    <image src="{{item.avatarUrl}}" class="friends-avatarUrl" />
    <text class="nickNameText" style="top:{{item.nickname.length>13?30:50}}rpx;">{{item.nickname}}</text>
    <text class="historyText">{{item.info}}</text>
    <view class="navigator" >
        <text>></text>
    </view>
</view>